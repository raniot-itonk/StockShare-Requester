{
  "Serilog": {
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://elasticsearch:9200/",
          "indexFormat": "log-{0:yyyy.MM}",
          "autoRegisterTemplate": true,
          "emitEventFailure": "WriteToSelfLog"
        }
      }
    ]
  },
  "Services": {
    "BankService": {
      "BaseAddress": "http://bankservice/"
    },
    "AuthorizationService": {
      "BaseAddress": "https://authorizationservice/"
    }
  }
}
